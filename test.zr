let array = [1, 2, 3, 4, 5];
print(array[1..-1]); // 2, 3, 4

print(array[1..3]); // 2, 3
print(array[1..=3]); // 2, 3, 4
print(array[..3]); // 1, 2, 3
print(array[..-1]); // 1, 2, 3, 4
print(array[1..]); // 2, 3, 4, 5
print(array[-1..0]); // 4, 3, 2
print(array[-1..=0]); // 4, 3, 2, 1
print(array[0..:2]); // 1, 3
print(array[0..=:2]); // 1, 3, 5
